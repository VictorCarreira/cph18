@article{Chakravarthy1999,
	abstract = {Delineation of bed boundaries is one of the important aspects of petrophysical interpretation. Proper location of bed boundaries plays a key role in log data inversion, especially in resistivity inversion, where the subsurface formation parameters are commonly parameterized into layers of varying thickness and resistivity. Traditionally, the location of bed boundaries is chosen based on the inflection points, maximum change in slope, etc. These algorithms work well for focussed responses and reasonably well for unfocussed responses, but the algorithms fail in presence of noise and in thin-bed regions. In order to overcome these difficulties, we propose an algorithm using Artificial Neural Networks (ANN) for detection of bed boundaries. We demonstrate the applicability of the algorithm on array induction data for a synthetic Oklahoma formation model and a field data from the Gulf of Mexico. The results obtained from the proposed scheme are compared with existing bed boundary algorithms},
	author = {Chakravarthy, S[] and Chunduru, R[] and Fanini, O[] and Mezzatesta, A[]},
	doi = {10.1190/1.1820779},
	file = {:home/vimana/Dropbox/Doutorado/Redes Neurais/chakravarthy1999.pdf:pdf},
	journal = {69th Ann. Internat. Mtg},
	keywords = {bed thickness,borehole geophysics,log analysis,neural networks,resistivity log},
	pages = {140--143},
	title = {{Detection of layer boundaries from array induction tool responses using neural networks}},
	year = {1999}
}
