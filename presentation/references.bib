@article{Moorkamp2013,
	abstract = {We present results of three-dimensional joint inversion of seismic, magnetotelluric, and gravity data over a marine salt dome. Such structures are difficult to image with a single method, and our results demonstrate how combining different techniques can yield improved results. More importantly, we examine the reliability of velocity-conductivity relationships derived from structure-coupled joint inversion approaches. Comparison with a seismic reflection section shows that our models match the upper limit of the salt. Furthermore, velocity and resistivity logs from a borehole drilled into the salt dome's flank match, within error, those recovered by the inversion. The good match suggests that the difference in length scale does not have a significant effect in this case. This provides a strong incentive to incorporate borehole data into the joint inversion in the future and substantiates approaches that use the relationships derived from joint inversion models for lithological classification.},
	author = {Moorkamp, M. and Roberts, A. W. and Jegen, M. and Heincke, B. and Hobbs, R. W.},
	doi = {10.1002/grl.50696},
	file = {:home/vimana/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Moorkamp et al. - 2013 - Verification of velocity-resistivity relationships derived from structural joint inversion with borehole data.pdf:pdf},
	issn = {00948276},
	journal = {Geophysical Research Letters},
	keywords = {gravimetry,joint inversion,magnetotellurics,parameter relationship,seismic tomography},
	number = {14},
	pages = {3596--3601},
	title = {{Verification of velocity-resistivity relationships derived from structural joint inversion with borehole data}},
	volume = {40},
	year = {2013}
}
@article{Fallis2013,
	abstract = {Predicting the binding mode of flexible polypeptides to proteins is an important task that falls outside the domain of applicability of most small molecule and protein−protein docking tools. Here, we test the small molecule flexible ligand docking program Glide on a set of 19 non-$\alpha$-helical peptides and systematically improve pose prediction accuracy by enhancing Glide sampling for flexible polypeptides. In addition, scoring of the poses was improved by post-processing with physics-based implicit solvent MM- GBSA calculations. Using the best RMSD among the top 10 scoring poses as a metric, the success rate (RMSD ≤ 2.0 {\AA} for the interface backbone atoms) increased from 21{\%} with default Glide SP settings to 58{\%} with the enhanced peptide sampling and scoring protocol in the case of redocking to the native protein structure. This approaches the accuracy of the recently developed Rosetta FlexPepDock method (63{\%} success for these 19 peptides) while being over 100 times faster. Cross-docking was performed for a subset of cases where an unbound receptor structure was available, and in that case, 40{\%} of peptides were docked successfully. We analyze the results and find that the optimized polypeptide protocol is most accurate for extended peptides of limited size and number of formal charges, defining a domain of applicability for this approach.},
	archivePrefix = {arXiv},
	arxivId = {arXiv:1011.1669v3},
	author = {Fallis, A.G},
	doi = {10.1017/CBO9781107415324.004},
	eprint = {arXiv:1011.1669v3},
	file = {:home/vimana/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Fallis - 2013 - No Title No Title(11).pdf:pdf},
	isbn = {9788578110796},
	issn = {1098-6596},
	journal = {Journal of Chemical Information and Modeling},
	keywords = {icle},
	number = {9},
	pages = {1689--1699},
	pmid = {25246403},
	title = {{No Title No Title}},
	volume = {53},
	year = {2013}
}
@article{Carreira2013,
	abstract = {This article presents a geologic-geophysical model along a 320 km NW – SE profile at the central region of Paran{\'{a}} basin, based primarily on a 2D gravity model, in complement to collocated magnetotelluric (MT) studies that are being carried out by the Brazilian National Agency of Petroleum, Natural Gas and Biofuels - ANP. The study of large structures within the basement underneath a sedimentary basin is of vital importance for the understanding of its tectonic-stratigraphic evolution. The Paran{\'{a}} basin is a shallow basin with a depocenter reaching approximately 7, 000m of estimated depth. Two large regional structures are remarkable in the study area: the Ponta Grossa Arch, a NW-SE direction feature; and the gravity high of Paranapanema, an elongated NW-SE positive anomaly. The region also includes large lava flows represented by basalt rocks of the Serra Geral Formation. Intrusion of dikes and sills of diabase, dating mainly from the Cretaceous, are equally striking in the region. These intrusions took advantage of certain bygone structural lineament groups to settle. The Bouguer surface of the study area was represented by a 4th degree polynomial, to estimate the depth of Moho. The proposed final gravimetric model includes the regional topography and presents good fit, with rms error of 1.7 mGal. Its shallowest part was obtained by taking into ac- count data from six existing stratigraphic wells in the area and also integrating 1D electrical resistivity model links from new magnetotelluric measured data in regional profile. The deepest structures of the model were connected to studies of deep seismic refraction, receptor function analysis and surface waves‘ dispersion, in particular the depth of the Moho. Tests with synthetic MT models support the proposed geological - geophysical model which present some notable characteristics: the crust – mantle interface is very smooth and oscillates around 42 km deep. The upper crust was represented by two blocks that correspond, in the northwestern portion, to the Precambrian continental crust and Paranapanema and, in the southeastern portion, to a mobile belt, also Precambrian, called Rio Apia{\'{i}}, characterized by positive residual gravity anomaly, in accordance with the tectonic context of the region already described in previous studies. The basin-basement limit, derived from the MT model, revealed a fractured surface with an average depth of 5.5 km. The sedimentary portion can be differentiated in its established super-sequences, but does not resolve the problematic of dikes and sills for the studied profile},
	annote = {Artigo do mestrado},
	archivePrefix = {arXiv},
	arxivId = {arXiv:1011.1669v3},
	author = {Carreira, Victor Ribeiro and {La Terra}, Emanuele Francesco and Fontes, Sergio Luis},
	doi = {10.1017/CBO9781107415324.004},
	eprint = {arXiv:1011.1669v3},
	file = {:home/vimana/Dropbox/Mestrado/Artigo/APPGEO4437.pdf:pdf},
	isbn = {9788578110796},
	issn = {1098-6596},
	journal = {Journal of Applied Geophysics},
	keywords = {icle},
	number = {9},
	pages = {1689--1699},
	pmid = {25246403},
	title = {{Density model for the central region of the Paran{\'{a}} basin, south portion of Brazil.}},
	volume = {53},
	year = {2013}
}
